{
  "name": "@areven/eslint-config",
  "author": "Areven",
  "license": "MIT",
  "version": "3.0.2",
  "description": "Areven's eslint config",
  "keywords": [
    "areven",
    "config",
    "eslint"
  ],
  "homepage": "https://areven.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/areven/config.git",
    "directory": "package/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/areven/config/issues"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "commonjs",
  "files": [
    "readme.md",
    "license.md",
    "changelog.md",
    "./dist/**/*",
    "!**/*.tsbuildinfo"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "raw": "./src/index.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "raw": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@areven/eslint-plugin": "2.0.2",
    "@stylistic/eslint-plugin": "5.2.2",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "globals": "16.3.0",
    "typescript-eslint": "8.38.0"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  }
}
