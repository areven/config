{
  "name": "@areven/eslint-config-next",
  "author": "Areven",
  "license": "MIT",
  "version": "2.0.1",
  "description": "Areven's eslint config for next",
  "keywords": [
    "areven",
    "config",
    "eslint"
  ],
  "homepage": "https://areven.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/areven/config.git",
    "directory": "package/eslint-config-next"
  },
  "bugs": {
    "url": "https://github.com/areven/config/issues"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "commonjs",
  "sideEffects": false,
  "files": [
    "readme.md",
    "license.md",
    "changelog.md",
    "./dist/**/*",
    "!**/*.tsbuildinfo"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "raw": "./src/index.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "raw": "./src/index.ts",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@areven/eslint-config": "^3.0.1",
    "@eslint/eslintrc": "3.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.3.0"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">= 15",
    "eslint": ">= 9",
    "next": ">= 15",
    "react": ">= 18"
  }
}
